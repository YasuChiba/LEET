<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leet.leet">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:name=".common.UtilCommon"
        android:allowBackup="true"
        android:icon="@mipmap/triton_web"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/triton_web"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:layout_gravity="center">
        <activity android:name=".screen.start.controller.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screen.main.controller.MainActivity" />
        <activity android:name=".screen.login.controller.LoginActivity" />
        <activity android:name=".screen.signup.controller.SignupActivity" />
    </application>

</manifest>